<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.everyClub">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Carpool_app"
        android:usesCleartextTraffic="true">
        <activity android:name="com.example.everyClub.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LandingActivity"></activity>

        <activity android:name=".MyClubActivity"></activity>
        <activity android:name=".ClubRegisterActivity"></activity>
        <activity android:name=".ClubListActivity"></activity>
        <activity android:name=".ClubDetailActivity"></activity>

        <activity android:name=".TableListActivity"></activity>
        <activity android:name=".TableRegisterActivity"></activity>
        <activity android:name=".TableDetailActivity"></activity>
    </application>

</manifest>